<s:SurfaceWindow x:Class="AppliProjetTut.SurfaceWindow1"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:s="http://schemas.microsoft.com/surface/2008"
    Title="AppliProjetTut"
    >
    <s:SurfaceWindow.Background>
        
       <ImageBrush ImageSource="./Resources/icon.png" />
        
    </s:SurfaceWindow.Background>
    <Grid Name="MainGrid">
        <s:TagVisualizer VisualizationAdded="OnVisualizationAdded" VisualizationRemoved="TagVisualizer_VisualizationRemoved">
            <s:TagVisualizer.Definitions>
                <s:TagVisualizationDefinition
                    Value="0x00"
                    LostTagTimeout="2000"
                    Source="./MenuPrincipal.xaml"
                    MaxCount="1"
                    
                    >
                </s:TagVisualizationDefinition>
            </s:TagVisualizer.Definitions>
        </s:TagVisualizer>
        <Canvas Name="NearParentEllipseCanvas" />
        <Grid Name="LinkParentLineGrid" />
        <Grid Name="LinkParentGrid" />
        <Grid Name="LineGrid" />
        <s:ScatterView Name="MainScatterView" Grid.Row="1" Grid.Column="0" />
    </Grid>
</s:SurfaceWindow>
